  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^4.1.6",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.1",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.2.1",
